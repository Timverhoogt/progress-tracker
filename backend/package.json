{
  "name": "progress-tracker-backend",
  "version": "1.0.0",
  "description": "Personal Progress Tracker Backend",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "migrate": "node dist/database/migrate.js",
    "cleanup-duplicates": "npx ts-node src/database/cleanup-duplicates.ts",
    "cleanup-duplicates:build": "npm run build && node dist/database/cleanup-duplicates.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "better-sqlite3": "^8.7.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "form-data": "^4.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "@sendgrid/mail": "^8.1.0",
    "node-cron": "^3.0.3",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/better-sqlite3": "^7.6.4",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.12",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.2",
    "ts-node-dev": "^2.0.0"
  }
}
